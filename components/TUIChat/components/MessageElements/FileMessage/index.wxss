.TUI-fileMessage {
  display: flex;
  padding: 10rpx 24rpx;
  background-color: #fff;
  border-radius: 2px 10px 10px 10px;
  border: 1px solid #D9D9D9;
}
.fileMessage{
  display: flex;
}
.fileMessage-box{
  display: flex;
  background: white;
  align-items: center;
  height: 150rpx;
}
.file-icon {
  width: 80rpx;
  height: 80rpx;
}
.file-title {
  max-width: 53vw;
  display: inline;
  word-wrap: break-word;
}
